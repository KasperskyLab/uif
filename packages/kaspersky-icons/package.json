{
  "name": "@kaspersky/icons",
  "version": "2.3.0",
  "description": "Kaspersky Design System Icon Pack",
  "author": "AO Kaspersky Lab",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kasperskylab/uif#readme",
  "repository": "kasperskylab/uif",
  "bugs": {
    "url": "https://github.com/kasperskylab/uif/issues"
  },
  "keywords": [
    "kaspersky",
    "open single management platform",
    "design system",
    "icon pack",
    "storybook"
  ],
  "module": "esm",
  "types": "index.d.ts",
  "tsConfig": "tsconfig.json",
  "sideEffects": false,
  "exports": {
    "./*": {
      "import": "./esm/*/index.js",
      "require": "./*/index.js"
    }
  },
  "typesVersions": {
    ">=1.5.1": {
      "*": [
        "./*/index.d.ts"
      ]
    }
  },
  "files": [
    "8/**/*",
    "12/**/*",
    "16/**/*",
    "24/**/*",
    "32/**/*",
    "40/**/*",
    "48/**/*",
    "index.d.ts",
    "index.js",
    "esm/**/*"
  ],
  "scripts": {
    "iconsArtifacts:clean": "rimraf index.* && rimraf 8/** && rimraf 12/** && rimraf 16/** && rimraf 24/** && rimraf 32/** && rimraf 40/** && rimraf 48/** && rimraf esm/**",
    "icons:clean": "rimraf src/index.ts && rimraf src/types/index.ts && rimraf src/8/** && rimraf src/12/** && rimraf src/16/** && rimraf src/24/** && rimraf src/32/** && rimraf src/40/** && rimraf src/48/**",
    "iconsSVGs:clean": "rimraf src/iconsSVGs/*/**",
    "icons:prepare": "node src/helpers/prepareIconsFolders.js",
    "icons:transform": "svgr --config-file ./svgr.config.js --out-dir src -- ./src/iconsSVGs",
    "icons:updateImports": "node src/helpers/updateImports.js",
    "icons:create": "npm run icons:clean && npm run iconsSVGs:clean && npm run icons:prepare && npm run icons:transform && npm run icons:updateImports",
    "icons:tsx-create": "npm run icons:clean && npm run icons:transform && npm run iconsArtifacts:clean && npm run icons:updateImports",
    "prepublishOnly": "npm run icons:tsx-create && rollup -c --bundleConfigAsCjs",
    "build": "npm run icons:create && npm run prepublishOnly",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run icons:tsx-create && jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run icons:tsx-create && storybook build -o storybook/icons"
  },
  "devDependencies": {
    "@kaspersky/dev-tools": "2.9.3",
    "@rollup/plugin-commonjs": "21.0.2",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-terser": "0.4.3",
    "@storybook/addon-essentials": "7.0.1",
    "@storybook/addon-interactions": "7.0.1",
    "@storybook/addon-links": "7.0.1",
    "@storybook/blocks": "7.0.1",
    "@storybook/react": "7.0.1",
    "@storybook/react-vite": "7.0.1",
    "@storybook/testing-library": "0.0.14-next.2",
    "@svgr/cli": "6.2.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.3.2",
    "@types/react": "18.2.42",
    "@types/react-modal": "3.16.0",
    "css-loader": "6.8.1",
    "eslint": "7.14.0",
    "eslint-plugin-storybook": "0.6.12",
    "jest": "27.5.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.68.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.31.2",
    "storybook": "7.0.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": "16.x || 17.x || 18.x",
    "react-dom": "16.x || 17.x || 18.x"
  },
  "overrides": {
    "@babel/runtime": "7.17.2",
    "is-core-module": "2.8.1",
    "json5": "2.2.0",
    "regenerator-runtime": "0.13.9",
    "resolve": "1.22.0"
  }
}