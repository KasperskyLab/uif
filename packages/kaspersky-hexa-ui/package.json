{
  "name": "@kaspersky/hexa-ui",
  "version": "6.323.10",
  "author": "AO Kaspersky Lab",
  "description": "Kaspersky Hexa UI Design System UI Kit",
  "license": "Apache-2.0",
  "homepage": "https://github.com/KasperskyLab/uif#readme",
  "repository": "KasperskyLab/uif",
  "bugs": {
    "url": "https://github.com/KasperskyLab/uif/issues"
  },
  "keywords": [
    "kaspersky",
    "open single management platform",
    "design system",
    "ui kit",
    "storybook",
    "hexa ui"
  ],
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": "./src/index.js",
    "./src/*": "./src/index.js",
    "./helpers": "./helpers/index.js",
    "./helpers/*": "./helpers/index.js",
    "./helpers/localization/types": "./helpers/localization/types.js",
    "./design-system": "./design-system/index.js",
    "./design-system/*": "./design-system/index.js",
    "./design-system/global-style/*": "./design-system/global-style/index.js",
    "./design-system/global-style/styles.css": "./design-system/global-style/styles.css",
    "./design-system/theme/themes/dark/colors": "./design-system/theme/themes/dark/colors.js",
    "./design-system/theme/themes/light/colors": "./design-system/theme/themes/light/colors.js"
  },
  "scripts": {
    "build": "npm i && rollup -c --bundleConfigAsCjs && gulp removeUndesiredCode",
    "release": "npm run build && gulp deploy",
    "build-publish": "npm run build && cd ./esm && npm publish && gulp renamePackage && npm publish",
    "lint": "npm run lint-es && npm run lint-ts",
    "lint-es": "eslint",
    "lint-es:fix": "eslint --fix",
    "lint-ts": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci",
    "build-storybook": "storybook build",
    "test": "npm run lint && jest --transformIgnorePatterns --maxWorkers=1 --forceExit --logHeapUsage",
    "test:only": "jest --transformIgnorePatterns --maxWorkers=1 --forceExit --logHeapUsage",
    "test:watch": "jest --watch",
    "jest:screenshots": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js",
    "jest:screenshots-update": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js -u",
    "test:screenshots": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots",
    "test:screenshots-update": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots-update",
    "changelog": "conventional-changelog -n ./.changelog/changelog.config.js -i Changelog.mdx -s -r 1 -t \"product/osmp/@kaspersky/hexa-ui@\"",
    "tag": "node .changelog/scripts/create-tag.mts",
    "changelog:ai": "node .changelog/scripts/changelogs-ai.mts"
  },
  "sideEffects": [
    "*.less",
    "design-system/assets/*",
    "design-system/global-style/*",
    "helpers/localization/i18next.js"
  ],
  "overrides": {
    "chokidar": "3.5.3",
    "es5-ext": "0.10.53",
    "fsevents": "2.3.2",
    "glob-watcher": "5.0.5",
    "react-element-to-jsx-string": "14.3.2",
