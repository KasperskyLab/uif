{
  "name": "@kaspersky/hexa-ui",
  "version": "6.197.0",
  "description": "Kaspersky Hexa UI Design System UI Kit",
  "author": "AO Kaspersky Lab",
  "license": "Apache-2.0",
  "homepage": "https://github.com/kasperskylab/uif#readme",
  "repository": "kasperskylab/uif",
  "bugs": {
    "url": "https://github.com/kasperskylab/uif/issues"
  },
  "keywords": [
    "kaspersky",
    "open single management platform",
    "design system",
    "ui kit",
    "storybook"
  ],
  "main": "src/index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "build": "npm i && gulp clear && gulp build && tsc --project tsconfig.build.json --outDir ./esm && tsc-alias -v --dir ./esm --project tsconfig.build.json",
    "release": "npm run build && gulp deploy",
    "build-publish": "npm run build && cd ./esm && npm publish && gulp renamePackage && npm publish --access public",
    "lint": "npm run lint-es && npm run lint-ts",
    "lint-es": "eslint ./src",
    "lint-es:fix": "eslint --fix ./src",
    "lint-ts": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci",
    "build-storybook": "storybook build",
    "test": "npm run lint && node --expose-gc ./node_modules/.bin/jest --transformIgnorePatterns --maxWorkers=1 --forceExit --logHeapUsage",
    "test:watch": "jest --watch",
    "jest:screenshots": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js",
    "jest:screenshots-update": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js -u",
    "test:screenshots": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots",
    "test:screenshots-update": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots-update"
  },
  "sideEffects": [
    "*.less",
    "design-system/assets/*",
    "design-system/global-style/*",
    "helpers/localization/i18next.js"
  ],
  "overrides": {
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "chokidar": "3.5.3",
    "es5-ext": "0.10.53",
    "eslint-plugin-jest": "27.5.0",
    "eslint-plugin-promise": "5.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "fsevents": "2.3.2",
    "glob-watcher": "5.0.5",
    "react-element-to-jsx-string": "14.3.2"
